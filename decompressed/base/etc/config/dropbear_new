
config dropbear 'lan'
	option IdleTimeout '600'
	option PasswordAuth 'on'
	option Port '22'
	option Interface 'lan'
	option RootPasswordAuth 'on'
	option enable '1'

config dropbear 'wan'
	option enable '0'
	option IdleTimeout '600'
	option PasswordAuth 'on'
	option Port '22'
	option Interface 'wan'

